{
  "name": "electron-react-skeleton",
  "description": "Electron application skeleton based on React, Webpack.",
  "version": "0.0.1",
  "main": "./main.js",
  "build": {
    "productName": "Electron-Skeleton",
    "companyName": "Electron-Skeleton",
    "appBundleId": "com.example.electron-skeleton"
  },
  "license": "MIT",
  "author": {
    "name": "Svetli Nikolov",
    "email": "me@svetli.name",
    "url": "https://github.com/svetli"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/svetli/electron-react-skeleton"
  },
  "keywords": [
    "electron",
    "typescript",
    "react"
  ],
  "engines": {
    "node": ">= 7",
    "yarn": ">= 1.2.0",
    "npm": ">= 4"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=development node ./scripts/start",
    "start:prod": "cross-env NODE_ENV=production node ./scripts/start",
    "compile:dev": "cross-env NODE_ENV=development webpack --config ./webpack/webpack.development.js",
    "compile:prod": "cross-env NODE_ENV=production webpack --config ./webpack/webpack.production.js",
    "build:dev": "yarn compile:dev && cross-env NODE_ENV=development node ./scripts/build",
    "build:prod": "yarn compile:prod && cross-env NODE_ENV=production node ./scripts/build",
    "test": "cross-env TEST_ENV=1 xvfb-maybe electron-mocha -t 10000 --renderer --require ts-node/register app/test/*.{ts,tsx} app/test/**/*.{ts,tsx}",
    "eslint": "node ./scripts/eslint",
    "eslint:fix": "yarn eslint --fix",
    "eslint:check": "eslint --print-config .eslintrc.* | eslint-config-prettier-check",
    "tslint": "tslint ./app/{src,test}/**/*.{ts,tsx}",
    "tslint:fix": "yarn tslint --fix",
    "lint": "yarn tslint && yarn eslint:check && yarn eslint",
    "lint:fix": "yarn tslint:fix && yarn eslint:fix",
    "cleanup": "rimraf build dist node_modules"
  },
  "devDependencies": {
    "@types/chai": "^4.1.2",
    "@types/mocha": "^2.2.47",
    "@types/node": "^9.4.0",
    "@types/react": "^16.0.35",
    "@types/react-dom": "^16.0.3",
    "@types/react-hot-loader": "^3.0.5",
    "awesome-typescript-loader": "^3.4.1",
    "babel-core": "^6.26.0",
    "babel-minify": "^0.2.0",
    "babel-webpack-plugin": "^0.1.1",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "chalk": "^2.3.0",
    "cross-env": "^5.1.3",
    "css-loader": "^0.28.9",
    "electron": "^1.7.11",
    "electron-devtools-installer": "^2.2.3",
    "electron-mocha": "^5.0.0",
    "electron-packager": "^10.1.2",
    "eslint": "^4.16.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-prettier": "^2.5.0",
    "eslint-plugin-react": "^7.6.0",
    "eslint-plugin-typescript": "^0.8.1",
    "express": "^4.16.2",
    "extract-text-webpack-plugin": "^3.0.2",
    "fs-extra": "^5.0.0",
    "html-webpack-plugin": "^2.30.1",
    "mocha": "^5.0.0",
    "mri": "^1.1.0",
    "node-sass": "^4.7.2",
    "prettier": "^1.10.2",
    "react-hot-loader": "^3.1.3",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.20.1",
    "ts-node": "^4.1.0",
    "tslint": "^5.9.1",
    "tslint-config-prettier": "^1.6.0",
    "tslint-microsoft-contrib": "^5.0.2",
    "tslint-react": "^3.4.0",
    "typescript": "^2.6.2",
    "typescript-eslint-parser": "^12.0.0",
    "webpack": "^3.10.0",
    "webpack-dev-middleware": "^2.0.4",
    "webpack-hot-middleware": "^2.21.0",
    "webpack-merge": "^4.1.1",
    "xvfb-maybe": "^0.2.1"
  },
  "dependencies": {
    "react": "^16.2.0",
    "react-dom": "16.2.0"
  }
}
